{
  "name": "rev-stencil",
  "private": true,
  "version": "0.0.1",
  "description": "rev-stencil",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build && stencil build",
    "run:ios": "cap run ios",
    "run:android": "cap run android",
    "start": "stencil build --dev --watch --serve --no-open",
    "start.prod": "stencil build --prod --watch --serve --no-open",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watch",
    "g": "stencil generate"
  },
  "devDependencies": {
    "@capacitor/cli": "latest",
    "@ionic/core": "next",
    "@stencil/core": "latest",
    "@types/jest": "latest",
    "jest": "latest",
    "jest-cli": "latest",
    "puppeteer": "latest",
    "workbox-build": "latest"
  },
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "@capacitor-community/electron": "latest",
    "@capacitor/android": "latest",
    "@capacitor/app": "latest",
    "@capacitor/core": "latest",
    "@capacitor/filesystem": "latest",
    "@capacitor/haptics": "latest",
    "@capacitor/ios": "latest",
    "@capacitor/keyboard": "latest",
    "@capacitor/status-bar": "latest",
    "@capacitor/storage": "latest",
    "@stencil/store": "latest",
    "typescript": "latest"
  }
